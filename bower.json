var obj = {
  days:parseInt(str.match(/ *(\d+)([d])/gm) || 0 ) ,
  hours: parseInt(str.match(/ *(\d+)([h])/gm) || 0 ) ,
  min: parseInt(str.match(/ *(\d+)([m])/gm) || 0 ) ,
  sec: parseInt(str.match(/ *(\d+)([s])/gm) || 0)
}
var allSec = (obj.days * 60 * 60 * 8) + (obj.hours * 60 * 60) + (obj.min * 60) + obj.sec
var days = parseInt( allSec/(60 * 60 * 8) )
var hours = parseInt((allSec - (days * 60 * 60 * 8))/(60 * 60))
var min = parseInt((allSec - ((days * 60 * 60 * 8) + (hours * 60 * 60)))/60)
var sec  = parseInt(allSec - ((days * 60 * 60 * 8) + (hours * 60 * 60) + (min *60)))

